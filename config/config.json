{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "liberty-pages-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/libertyPages/LibertyPagesWebPart.js",
          "manifest": "./src/webparts/libertyPages/LibertyPagesWebPart.manifest.json"
        }
      ]
    },
    "task-list-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/taskListWebpart/TaskListWebpartWebPart.js",
          "manifest": "./src/webparts/taskListWebpart/TaskListWebpartWebPart.manifest.json"
        }
      ]
    },
    "recommended-articles-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/recommendedArticles/RecommendedArticlesWebPart.js",
          "manifest": "./src/webparts/recommendedArticles/RecommendedArticlesWebPart.manifest.json"
        }
      ]
    },
    "lm-app-customizer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/lmAppCustomizer/LmAppCustomizerApplicationCustomizer.js",
          "manifest": "./src/extensions/lmAppCustomizer/LmAppCustomizerApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "LibertyPagesWebPartStrings": "lib/webparts/libertyPages/loc/{locale}.js",
    "LmAppCustomizerApplicationCustomizerStrings": "lib/extensions/lmAppCustomizer/loc/{locale}.js",
    "TaskListWebpartWebPartStrings": "lib/webparts/taskListWebpart/loc/{locale}.js",
    "RecommendedArticlesWebPartStrings": "lib/webparts/recommendedArticles/loc/{locale}.js"
  }
}
